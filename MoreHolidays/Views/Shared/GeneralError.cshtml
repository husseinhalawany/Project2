@model BusinessLogic.Model.ReloadURLModel
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";

}




@section MainHeader{

    @Html.Partial("PartialViews/PartialAdminHeader")
}

@section Container{
<div class="error_page row text-center">
    <div class="col-lg-12">
        <img src="~/Content/MoreHoliday/image/error.png" />
    </div>
    <div class="col-lg-12">
@if (string.IsNullOrEmpty(Model.Message))
{
        <h1> Oops! That Page Couldn't be Found. </h1>
        <a href="@Url.Action("Index","Home")">Go Home</a><br />
        <a href=@Model.URL>Reload Page</a>
}
else
{
    <h1> Oops! @Model.Message </h1>
    <a href="@Url.Action("Index","Home")">Go Home</a><br />
    <a href=@Model.URL>Reload Page</a>
}
    </div>

</div>
    
}

@section MainFooter{

    @Html.Partial("PartialViews/PartialAdminFooter")
}
@section JavaScriptSection{

               }


