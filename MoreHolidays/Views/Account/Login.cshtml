            <div class="signform col-lg-6 col-md-12 col-xs-12">
                <div class="login-wrap">
                    <div class="login-html">
                        <span class="sign-in">LogIn</span>
                        <div class="login-form">
                            <div class="sign-in-htm">

                                @using (Html.BeginForm("Login", "Account", FormMethod.Post))
                                {

                                    @Html.AntiForgeryToken()

                                    <div class="group">
                                        @Html.TextBoxFor(m => m.UserName, new { @class = "input", placeholder = "Enter Username" })
                                    </div>

                                    <div class="group">
                                        @Html.PasswordFor(m => m.Password, new { @class = "input", placeholder = "Enter Password" })
                                    </div>

                                    <div class="group">
                                        <button type="submit" class="button">Login</button>
                                    </div>
                                    <div class="group">
                                        <input type="checkbox" class="rememer">
                                        <label class="rememer"> Remember Me</label>
                                    </div>

                                    <div class="hr"></div>
                                    <div class="foot-lnk">
                                         <a href="#forgot">Forgot Password?</a>
                                    </div>
                                }

                            </div>

                        </div>

                    </div>
                </div>
            </div>

        </div>

